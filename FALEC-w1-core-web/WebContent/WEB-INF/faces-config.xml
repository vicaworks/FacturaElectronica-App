<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
    
    <name>farlecpv_config</name>
    
    <component>
        <component-type>org.primefaces.component.RomaMenu</component-type>
        <component-class>org.primefaces.roma.component.RomaMenu</component-class>
    </component>
    
    <render-kit>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.RomaMenuRenderer</renderer-type>
            <renderer-class>org.primefaces.roma.component.RomaMenuRenderer</renderer-class>
        </renderer>
    </render-kit>
    
    <application>
		<action-listener>org.omnifaces.eventlistener.ResetInputAjaxActionListener</action-listener>

		<locale-config>
			<default-locale>es</default-locale>
		</locale-config>

		<resource-bundle>
			<base-name>com.vcw.falec.web.resource.MessageResourcesWeb_es</base-name>
			<var>msg</var>
		</resource-bundle>
	</application>
	
	

</faces-config>
